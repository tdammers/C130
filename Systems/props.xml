<PropertyList>

    <!-- PROP 0 (#1 engine) -->
    <filter>
        <type>gain</type>
        <gain>1</gain>
        <input>
            <expression>
                <difference>
                    <mod>
                        <sum>
                            <property>/engines/engine[0]/rpm</property>
                            <value>225</value>
                        </sum>
                        <value>450</value>
                    </mod>
                    <value>225</value>
                </difference>
            </expression>
        </input>
        <output>
            <property>/engines/engine[0]/observed-rpm</property>
        </output>
    </filter>

