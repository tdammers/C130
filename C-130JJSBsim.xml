          <table>
            <independentVar lookup="row">aero/alpha-rad</independentVar>
            <tableData>
             -1.57    1.500
             -0.26    0.023
              0.00    0.005
              0.26    0.025
              1.57    0.500
            </tableData>
          </table>
       </product>
    </function>

    <function name="aero/force/Drag_induced">
